	<xs:simpleType name="WallConstructionType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="brick"/>
			<xs:enumeration value="concrete"/>
			<xs:enumeration value="quarrystone"/>
			<xs:enumeration value="rock"/>
		</xs:restriction>
