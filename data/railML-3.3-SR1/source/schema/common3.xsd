		</xs:annotation>
		
		<xs:attribute name="yValue" use="required" type="xs:decimal">
			<xs:annotation>
				<xs:documentation>value of ordinate (y-axis) for this line (row)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
