<xs:schema targetNamespace="https://www.railml.org/schemas/3.3" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:rail3="https://www.railml.org/schemas/3.3" xmlns:dcterms="http://purl.org/dc/terms/" elementFormDefault="qualified" version="3.3">
